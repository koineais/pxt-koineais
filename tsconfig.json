{
    "compilerOptions": {
        "target": "ES5",
        "module": "commonjs",
        "lib": ["ES2015"],
        "declaration": true,
        "outDir": "built",
        "rootDir": ".",
        "strict": true,
        "allowJs": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true
    },
    "exclude": ["node_modules", "built"]
}
